{
  "name": "@warren-bank/Android-AirPlay-Client",
  "private": true,
  "license": "GPL-3.0",
  "devDependencies": {
    "@warren-bank/translate-android-strings": "^3.0.1"
  },
  "scripts": {
    "translate":       "translate-android-strings -s \"deepl\" -i \"en\" -f \"android-studio-project/AirPlay-Client/src/main/res/values/strings.xml\" -d \"android-studio-project/AirPlay-Client/src/withAllLanguageTranslations/res\" -m --na --nc --nw -b \"(?:DroidPlay|AirPlay|JPEG|[\\\\][uU][\\d]{4}|[\\d]+|\\s+\\[)\"",
    "translate:debug": "npm run translate -- -o \"en\" --debug"
  }
}
